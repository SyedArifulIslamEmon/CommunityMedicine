@model IEnumerable<CommunityMedicine.Models.Login>

@{
    ViewBag.Title = "Login Form";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Login Form</h2>
@{
    ViewBag.Title = "Login Form";
}

<form action="CenterReport">
    <div class="form-group">
        <label for="CenterCode">Enter Code</label>
        <input type="text" class="form-control" id="CenterCode" name="CenterCode" placeholder="Enter Code">
    </div>
    <div class="form-group">
        <label for="Password">Password</label>
        <input type="password" class="form-control" id="Password" name="Password" placeholder="Password">
    </div>

    <button type="submit" class="btn btn-default">Login</button>
</form>



